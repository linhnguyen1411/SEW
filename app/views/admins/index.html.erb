<h1 style="text-align: center;">Welcome to admin page!</h1>
<div class="margin-bottom-50"><hr></div>
<div class="row">
  <div class="counters col-md-3 col-sm-3">
    <span class="counter-icon"><i class="fa fa-user rounded"></i></span>
    <span class="counter"><%= count_user %></span><h4 style="color: red">Users</h3>
  </div>
  <div class="counters col-md-3 col-sm-3">
    <span class="counter-icon"><i class="fa fa-rss rounded"></i></span>
    <span class="counter"><%= count_news %></span><h4 style="color: green">News</h3>
  </div>
  <div class="counters col-md-3 col-sm-3">
    <span class="counter-icon"><i class="fa fa-eye rounded"></i></span>
    <span class="counter">1048299</span><h4 style="color: blue">Views</h3>
  </div>
  <div class="counters col-md-3 col-sm-3">
    <span class="counter-icon"><i class="fa fa-pencil-square-o rounded"></i></span>
    <span class="counter">23892</span><h4 style="color: #c540de">Blogs</h3>
  </div>
</div>
<div class="margin-bottom-50"><hr></div>
<div class="row margin-bottom-40">
  <div class="counters col-md-3 col-sm-3">
      <span class="counter"><%=count_ctg%></span>
      <h4>Category Groups</h4>
  </div>
  <div class="counters col-md-3 col-sm-3">
      <span class="counter"><%= count_category %></span>
      <h4>Categories</h4>
  </div>
  <div class="counters col-md-3 col-sm-3">
      <span class="counter"><%=count_site%></span>
      <h4>Site Resources</h4>
  </div>
  <div class="counters col-md-3 col-sm-3">
      <span class="counter"><%= count_rss %></span>
      <h4>Rss links</h4>
  </div>
</div>
<script type="text/javascript">
    jQuery(document).ready(function() {
        App.init();
        App.initCounter();
    });
</script>
